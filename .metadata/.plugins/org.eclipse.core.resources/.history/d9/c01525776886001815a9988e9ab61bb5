package com.fil.java.jdbc;

import java.util.List;

import com.fil.dao.SupplierDao;
import com.fil.daoimpl.SupplierDaoImpl;
import com.fil.model.Supplier;



public class DaoDemo {

	public static void main(String[] args){
		
		SupplierDao dao = new SupplierDaoImpl();
		List<Supplier> list = dao.getAllSuppliers();
		for(Supplier s : list){
			System.out.println(s);
		}
		Supplier obj = new Supplier(11,"Screw","Blue");
		dao.addSupplier(obj);
		list = dao.getAllSuppliers();
		for(Supplier s : list){
			System.out.println(s);
		}
		dao.deleteSupplier(obj);
		list = dao.getAllSuppliers();
		for(Supplier s : list){
			System.out.println(s);
		}
		
	}

}

package com.fil.java.jdbc;

import java.Dao.SupplierDao;
import java.DaoImpl.SupplierDaoImpl;
import java.Model.Supplier;
import java.util.List;

public class DaoDemo {

	public static void main(String[] args){
		
		SupplierDao dao = new SupplierDaoImpl();
		List<Supplier> list = dao.getAllSuppliers();
		for(Supplier s : list){
			System.out.println(s);
		}
		Supplier obj = new Supplier(10,"Hammer","Black");
		dao.addSupplier(obj);
		list = dao.getAllSuppliers();
		for(Supplier s : list){
			System.out.println(s);
		}
		dao.deleteSupplier(obj);
		list = dao.getAllSuppliers();
		for(Supplier s : list){
			System.out.println(s);
		}
		
	}

}
